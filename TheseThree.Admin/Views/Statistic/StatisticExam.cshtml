
<div class="panel-body" style="padding-bottom: 0;">
    <div style="margin-bottom: 5px;">
        <ul class="breadcrumb " style="margin-bottom:0;" data-dojo-attach-point="breadcrumb_1">
            <li><a href="#" style="color:#303f9f;font-size:1.1em;" data-dojo-attach-point="item_1">统计报表</a></li>
            <li><a data-ajax="true" data-ajax-mode="replace" data-ajax-update="#main" href="/Statistic/INdex/" style="color: #303f9f; font-size: 1.1em;" data-dojo-attach-point="item_1">考试统计</a></li>
            <li class="active" style="color:#757575;font-size:1.1em;" data-dojo-attach-point="item_3">考试详情</li>
        </ul>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">查询条件</div>
        <div class="panel-body">
            <form class="form-inline">
                <input type="hidden" id="hid_exam_id" value="@ViewBag.eid"/>
                <div class="form-group">
                    <div class="input-group">
                        <div class="input-group-addon">类型</div>
                        <select class="form-control" id="sel_level" style="width: 230px;">
                            <option value="0">院级</option>
                            <option value="1">科级</option>
                        </select>
                    </div>
                    <div class="input-group">
                        <div class="input-group-addon">科室</div>
                        <select class="form-control" id="sel_dept" style="width: 230px;">
                            <option value="0">全部</option>
                            @{
                                if (ViewBag.ks != null)
                                {
                                    foreach (var k in ViewBag.ks)
                                    {
                                        <option value="@k.Value">@k.Name</option>
                                    }
                                }
                            }
                        </select>
                    </div>
                    <div class="input-group">
                        <div class="input-group-addon">月份</div>
                        <select class="form-control" id="sel_month" style="width: 230px;">
                            <option value="0">全部</option>
                            @{
                                if (ViewBag.monthes != null)
                                {
                                    foreach (var k in ViewBag.monthes)
                                    {
                                        <option value="@k">@k</option>
                                    }
                                }
                            }
                        </select>
                    </div>
                    <div class="input-group">
                        <div class="input-group-addon">姓名</div>
                        <input type="text" class="form-control" id="txt_name" placeholder="姓名" style="width: 230px;">
                    </div>
                    <div class="input-group">
                        <div class="input-group-addon">工号</div>
                        <input type="text" class="form-control" id="txt_loginid" placeholder="工号" style="width: 230px;">
                    </div>
                    <div class="input-group">
                        <button type="button" id="btn_query" class="btn btn-primary" style="width: 100px;">查询</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div id="toolbar" class="btn-group">
        <button id="btn_export_xls" type="button" class="btn btn-default">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>导出Excel
        </button>
        <button id="btn_export_word" type="button" class="btn btn-default">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>导出Word
        </button>
    </div>
    <table id="staticExamTable"></table>
</div>
<script src="~/Scripts/statistic_detail_exam.js"></script>